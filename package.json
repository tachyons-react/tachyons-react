{
  "name": "tachyons-react",
  "author": "John Otander",
  "version": "1.0.0",
  "description": "Tachyons React components",
  "main": "index.js",
  "nyc": {
    "exclude": [
      "other"
    ]
  },
  "scripts": {
    "cover": "nyc --reporter=lcov --reporter=text --reporter=html npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test": "ava 'src/**/*/test.js' --verbose --require ./other/setup-ava-tests.js",
    "watch:test": "nodemon --quiet --watch app --exec npm run test -s",
    "watch:cover": "nodemon --quiet --watch app --exec npm run cover -s"
  },
  "repository": "tachyons-react/tachyons-react",
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "ava": "0.9.2",
    "babel-core": "6.5.1",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.5.1",
    "jsdom": "8.0.2",
    "nodemon": "1.8.1",
    "nyc": "5.6.0",
    "react": "0.14.7",
    "react-addons-test-utils": "0.14.7",
    "react-dom": "0.14.7",
    "sinon": "1.17.3"
  }
}
